{
  "name": "storybook-addon-knobs-compat",
  "version": "0.0.1",
  "description": "Flexible story control paramaterization",
  "keywords": [
    "controls",
    "knobs",
    "storybook-addons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nickofthyme/storybook-knobs-compat"
  },
  "type": "module",
  "license": "MIT",
  "author": "Nick Partridge <nick.ryan.partridge@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "yarn build --watch",
    "start": "run-p build:watch \"storybook --quiet\"",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "yarn build && auto shipit",
    "release:dry": "yarn build && auto shipit --dry-run",
    "storybook": "storybook dev -p 6006",
    "storybook:dev": "onchange -i -k './src/**/*' -- yarn storybook --ci",
    "storybook:build": "storybook build",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@storybook/icons": "1.4.0",
    "ts-debounce": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/css": "^0.10.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.2.0",
    "@storybook/addon-docs": "^9.0.0",
    "@storybook/react-vite": "^9.0.0",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/node": "^22",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^4.2.1",
    "auto": "^11.1.1",
    "boxen": "^7.1.1",
    "eslint": "^9.34.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-oxlint": "^1.13.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jiti": "^2.5.1",
    "npm-run-all2": "^6.2.3",
    "onchange": "^7.1.0",
    "prettier": "^3.3.3",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^9.0.0",
    "ts-dedent": "^2.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^5.3.5",
    "zx": "^8.4.1"
  },
  "peerDependencies": {
    "storybook": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.ts"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "Addon Knobs Compat",
    "supportedFrameworks": [
      "react"
    ]
  }
}
